{
  "$defs": {
    "Action": {
      "oneOf": [
        {
          "required": [
            "keep_only_transactions"
          ]
        },
        {
          "required": [
            "rename_account"
          ]
        }
      ],
      "properties": {
        "keep_only_transactions": {
          "description": "Drop every directive except transactions",
          "title": "Keep Only Transactions",
          "type": "boolean"
        },
        "rename_account": {
          "allOf": [
            {
              "$ref": "#/$defs/RenameAccount"
            }
          ],
          "description": "Rename accounts"
        }
      },
      "title": "Action",
      "type": "object"
    },
    "BeancountPluginConfig": {
      "properties": {
        "module_name": {
          "description": "Plugin of beancount ecosystem (e.x. beancount.plugins.auto_accounts)",
          "examples": "beancount.plugins.auto_accounts",
          "title": "Module Name",
          "type": "string"
        },
        "string_config": {
          "default": null,
          "title": "String Config",
          "type": "string"
        }
      },
      "required": [
        "module_name"
      ],
      "title": "BeancountPluginConfig",
      "type": "object"
    },
    "RenameAccount": {
      "properties": {
        "src": {
          "title": "Src",
          "type": "string"
        },
        "dest": {
          "title": "Dest",
          "type": "string"
        }
      },
      "required": [
        "src",
        "dest"
      ],
      "title": "RenameAccount",
      "type": "object"
    }
  },
  "properties": {
    "plugins": {
      "default": [],
      "items": {
        "anyOf": [
          {
            "$ref": "#/$defs/Action"
          },
          {
            "$ref": "#/$defs/BeancountPluginConfig"
          }
        ]
      },
      "title": "Plugins",
      "type": "array"
    }
  },
  "title": "RootConfig",
  "type": "object"
}
